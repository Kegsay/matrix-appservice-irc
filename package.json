{
  "name": "matrix-appservice-irc",
  "version": "0.1.0",
  "description": "IRC Bridge for matrix-appservice",
  "main": "index.js",
  "scripts": {
    "test": "istanbul cover --report cobertura jasmine-node -- spec --verbose --junitreport --forceexit --captureExceptions",
    "testonly": "jasmine-node spec --verbose --forceexit --captureExceptions",
    "lint": "jshint -c .jshint lib spec && gjslint --unix_mode --disable 0131,0220 --max_line_length 90 -r lib/ -r spec/",
    "check": "npm test && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matrix-org/matrix-appservice-irc.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/matrix-org/matrix-appservice-irc/issues"
  },
  "dependencies": {
    "crc": "^3.2.1",
    "extend": "^2.0.0",
    "fs": "0.0.2",
    "irc": "matrix-org/node-irc",
    "jayschema": "^0.3.1",
    "js-yaml": "^3.2.7",
    "matrix-appservice": "0.1.0",
    "matrix-js-sdk": "0.2.0",
    "nedb": "^1.1.2",
    "nopt": "^3.0.1",
    "q": "^1.2.0",
    "request": "^2.54.0",
    "sanitize-html": "^1.6.1",
    "winston": "^0.9.0"
  },
  "devDependencies": {
    "istanbul": "^0.3.13",
    "proxyquire": "^1.4.0",
    "jasmine-node": "^1.14.5",
    "jshint": "^2.8.0"
  }
